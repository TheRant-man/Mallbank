<?xml version="1.0" ?>
<dynamic_object>
	<diesel materials="units/payday2/architecture/bnk/bnk_int" orientation_object="rp_bnk_int_fence_door" />
	<sequence_manager file="units/payday2/architecture/bnk/bnk_int_fence_door" />
	<animations>
		<animation_group name="anim" enabled="false">
			<object name="anim_door" />
		</animation_group>
	</animations>

	<bodies>
		<body name="body_door_whole" enabled="true" template="animated">
			<object name="anim_door" />
			<object name="c_door" collision_type="mesh_mopp" />
		</body>
		<body name="body_door_broken" enabled="false" template="static">
			<object name="anim_door" />
			<object name="c_door_expl" collision_type="mesh_mopp" />
		</body>
		<body name="hitbox" enabled="true" template="animated">
			<object name="anim_door" />
			<object name="c_door_lock_hitbox" collision_type="box" />
		</body>
	</bodies>

	<decal_surfaces default_material="metal_hollow">
	</decal_surfaces>

	<graphics>
		<graphic_group name="sawicongroup" 	enabled="true" culling_object="g_icon_saw">
			<lod_object name="saw_icon">
					<object name="g_icon_saw" 		enabled="true"  max_distance="450"  max_draw_lod="0" />
					<object name="g_icon_saw_lod1" 	enabled="true"  lod="1" />
			</lod_object>
		</graphic_group>
		<object name="g_door" 			enabled="true" shadow_caster="false" />
		<object name="s_door" 			enabled="true" shadow_caster="true" />
		
		<object name="g_door_drill" 	enabled="false" shadow_caster="false" />
		<object name="s_door_drill" 	enabled="false" shadow_caster="true" />

		<object name="g_door_expl" 		enabled="false" shadow_caster="false" />
		<object name="s_door_expl" 		enabled="false" shadow_caster="true" />
	</graphics>
</dynamic_object>